	ar1.add("aasdfsdf");
	ar1.add("asdfasdff");
	ar1.add(1,"manoj");
	ar1.get(1);
	ar=ar1;
	int arr[]={1,2,3};
	int arr1[]={1,2,3};
	try{
		arr1=arr;
		System.out.println("asdf");
		System.out.println(arr1[2]);
		}
	finally{
	System.out.println(ar.get(1));
		}
	Set s=new HashSet();
	System.out.println(arr==arr1);
	}
