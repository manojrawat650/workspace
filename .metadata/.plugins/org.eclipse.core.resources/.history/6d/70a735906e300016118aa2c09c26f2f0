package print;
import java.io.*;
import java.util.*;

public class ArbitrageDemo {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      sc.nextLine();
      float x;
      float profit=100000;
      String a[]=new String[3];
      float f[]=new float[n];
      for (int i=0;i<n;i++)
      {
      a=sc.nextLine().split(" ");
      for (int j=0;j<3;j++)
      {
    	  x=Float.parseFloat(a[j]);
    	profit=profit/x;  
      }
      if (profit>100000)
      {
    	  f[i]=profit-100000;
      }
      else
      {
    	  f[i]=0;
      }
	  }
      for (int l=0;l<n;l++)
      {
    	  System.out.println(f[l]);
      }
	}
}

