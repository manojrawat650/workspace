package Snippet;

import java.util.*;
public class Text
{
	public static void main(String args[])
	{	ArrayList<String> ar=new ArrayList<String>();
	ar.add("asdf");
	ar.add("asdff");
	ArrayList<String> ar1=new ArrayList<String>();
	ar1.add("aasdfsdf");
	ar1.add("asdfasdff");
	ar=ar1;
	int arr[]={1,2,3};
	int arr1[]={5,6,6,4};
	try{
		arr1=arr;
		System.out.println("asdf");
		System.out.println(arr1[3]);
		}
	finally{
	System.out.println(ar.get(1));
		}}
}
