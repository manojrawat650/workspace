
public class RotatedStringDemo {

	public static void main(String[] args) {
		char ch1[]={'s','t','r','i','n','g'};
		for (int i=0;i<ch1.length;i++)
		{   char temp;
			for (int j=0;j<ch1.length-1;j++)
			{   temp=ch1[0];
				ch1[j]=ch1[j+1];
				ch1[ch1.length-1]=temp;
				
			}
			System.out.println(ch1);
			
		}

	}

}
