import java.util.*;
public class ArrayListQuery {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int N=sc.nextInt();
		sc.nextLine();
		ArrayList<String[]> ar=new ArrayList<String[]>(N);
		for (int i=0;i<N;i++)
		{
			ar.add(sc.nextLine().split(" "));
		}
		int q=sc.nextInt();
		for (int i=0;i<q;i++)
		{
			String st[]=sc.nextLine().split(" ");
			System.out.println(ar.get(Integer.parseInt(st[0]))[Integer.parseInt(st[1])]);
		} 

	}

}
